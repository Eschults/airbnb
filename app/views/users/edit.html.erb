
<div class="col-xs-4">

  <%= bootstrap_form_for(current_user) do |f| %>

    <%= f.email_field :email, autofocus: true %>

    <%= f.text_field :first_name, autofocus: true %>
    <%= f.text_field :last_name, autocomplete: "off" %>

    <%= f.select :gender, [["Male", "Male"], ["Female", "Female"]], autocomplete: "off" %>

    <%= f.date_field :birthday, autocomplete: "off" %>
    <%= f.text_field :phone_number, autocomplete: "off" %>
    <%= f.text_field :description, autocomplete: "off" %>
    <%= f.submit "Update" %>

    <%= link_to "Change your password", edit_user_registration_path(current_user) %>

  <% end %>

</div>